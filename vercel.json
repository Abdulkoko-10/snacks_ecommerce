{
  "version": 2,
  "builds": [
    {
      "src": "orchestrator/src/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "connectors/serpapi-connector/src/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/api/v1/(.*)",
      "destination": "/orchestrator/src/index.ts"
    }
  ]
}