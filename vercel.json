{
  "version": 2,
  "builds": [
    {
      "src": "orchestrator/src/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/v1/:path*",
      "destination": "/orchestrator/src/index.ts"
    }
  ]
}