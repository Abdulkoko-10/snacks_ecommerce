# Sanity Project Details (Example - Replace with your actual keys)
NEXT_PUBLIC_SANITY_PROJECT_ID="your_sanity_project_id"
NEXT_PUBLIC_SANITY_TOKEN="your_sanity_read_token"
# Set this to false if you want to use the live Sanity dataset, or true for a development/staging dataset
NEXT_PUBLIC_SANITY_USE_CDN=false
# Used for connecting to Sanity with write access for reviews, etc. (Keep this secret)
SANITY_API_WRITE_TOKEN="your_sanity_write_token"

# Stripe API Keys (Example - Replace with your actual keys)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY="your_stripe_publishable_key"
STRIPE_SECRET_KEY="your_stripe_secret_key"

# Clerk Authentication
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY="your_clerk_publishable_key"
CLERK_SECRET_KEY="your_clerk_secret_key"
